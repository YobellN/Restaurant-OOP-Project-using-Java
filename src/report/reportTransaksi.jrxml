	<parameter name="id_pesanan" class="java.lang.String"/>
	<queryString>
		<![CDATA[SELECT 
    PL.nama_pelanggan, 
    K.nama_karyawan, 
    K.id_karyawan, 
    M.nama_menu, 
    M.jenis_menu, 
    M.id_menu, 
    M.harga, 
    T.id_pesanan, 
    T.tanggal_pesanan, 
    T.total_harga, 
    PS.jumlah, 
    PS.sub_total
FROM 
    Transaksi T
LEFT JOIN 
    Pelanggan PL ON T.id_pelanggan = PL.id_pelanggan
LEFT JOIN 
    Karyawan K ON T.id_karyawan = K.id_karyawan
LEFT JOIN 
    Pesanan PS ON T.id_pesanan = PS.id_pesanan
LEFT JOIN 
    Menu M ON PS.id_menu = M.id_menu
WHERE 
    T.id_pesanan = $P{id_pesanan}]]>
	</queryString>
